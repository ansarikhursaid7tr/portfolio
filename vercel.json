{
    "builds": [
        {
            "src": "dev_case/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize" : "15mb", "runtime": "python3.8"}
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config":{
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
          "src": "/static/(.*)",
          "dest": "/static/$1"
        },
        {
          "src": "/(.*)",
          "dest": "dev_case/wsgi.py"
        }
      ]
}